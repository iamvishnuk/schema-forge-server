  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.ts",
    "dev": "nodemon src/index.ts",
    "dev:docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down",
    "dev:docker:clean": "docker compose -f docker-compose.dev.yml down -v",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run src/__tests__/integration",
    "test:e2e": "vitest run src/__tests__/e2e",
    "test:containers:up": "docker compose -f docker-compose.test.yml up -d",
    "test:containers:down": "docker compose -f docker-compose.test.yml down",
    "test:containers:logs": "docker compose -f docker-compose.test.yml logs -f",
    "test:containers:clean": "docker compose -f docker-compose.test.yml down -v"
  },
  "dependencies": {
